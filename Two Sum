class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        for i in range(len(nums)):
            if i+1 < len(nums):
                j = i+1
            else:
                len(nums) - 1
            z = nums[i] + nums[j]
            if z == target:
                return [i,j]
